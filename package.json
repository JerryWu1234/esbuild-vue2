{
  "name": "esbuild-vue2",
  "version": "1.0.0",
  "packageManager": "pnpm@7.1.1",
  "description": "build bundle of vue2 in esbuild",
  "keywords": [
    "esbuild",
    "esbuild-plugin",
    "plugin-esbuild"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dd": "node ./d.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "^17.0.34",
    "@vue/component-compiler": "~4.2.4",
    "@vue/component-compiler-utils": "~3.3.0",
    "bumpp": "^7.1.1",
    "cypress": "^10.0.3",
    "esbuild": "^0.14.47",
    "eslint": "^8.15.0",
    "fast-glob": "^3.2.11",
    "hash-sum": "^2.0.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.0",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6",
    "vue": "~2.6.14",
    "vue-template-compiler": "~2.6.14"
  },
  "homepage": "https://github.com/wulinsheng123/auto-monitor#readme",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulinsheng123/auto-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/wulinsheng123/auto-monitor/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue-template-compiler": "~2.6.14"
  }
}
